import java.util.Scanner;

public class Ex06_08 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Scanner sc = new Scanner(System.in);
        
        int A = sc.nextInt();
        int B = sc.nextInt();
        
        if(A>0 && B<10)
            System.out.printf("%.1f", A/(float)B);
	}
	
	

}
